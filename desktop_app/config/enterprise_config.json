{
  "enterprise": {
    "enabled": true,
    "features": {
      "siem_integration": true,
      "ai_detection": true,
      "cloud_monitoring": false,
      "compliance_reporting": true
    },
    "siem": {
      "type": "splunk",
      "splunk": {
        "host": "localhost",
        "port": 8088,
        "token": "your-splunk-token-here",
        "index": "cybersnoop"
      },
      "elastic": {
        "host": "localhost",
        "port": 9200,
        "username": "",
        "password": ""
      }
    },
    "ai": {
      "anomaly_detection": {
        "contamination": 0.1,
        "n_estimators": 100
      },
      "behavioral_analysis": {
        "enabled": true,
        "sensitivity": "medium"
      }
    },
    "compliance": {
      "reports": ["pci_dss", "hipaa", "gdpr"],
      "schedule": "daily",
      "retention_days": 365
    },
    "cloud": {
      "providers": {
        "aws": {
          "enabled": false,
          "access_key": "",
          "secret_key": "",
          "region": "us-east-1"
        },
        "azure": {
          "enabled": false,
          "subscription_id": "",
          "tenant_id": "",
          "client_id": "",
          "client_secret": ""
        }
      }
    }
  },
  "monitoring": {
    "interface": "auto",
    "capture_filter": "",
    "packet_buffer_size": 10000,
    "threat_detection": {
      "enabled": true,
      "sensitivity": "medium"
    }
  },
  "database": {
    "retention_days": 30,
    "max_size_mb": 1000,
    "backup_enabled": true
  },
  "notifications": {
    "system_tray": true,
    "email": {
      "enabled": false,
      "smtp_server": "",
      "smtp_port": 587,
      "username": "",
      "password": "",
      "recipients": []
    }
  }
}
